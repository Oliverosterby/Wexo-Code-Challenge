<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Wexo</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Challenge.styles.css" asp-append-version="true" />
</head>
 <body>
     <header>
         <nav class="navbar navbar-expand-lg">
             <div class="container-fluid">
                 <a class="navbar-brand" href="#">
                     <img src="https://uploads-ssl.webflow.com/5f7da9758e9dbca1e953c128/608a7c11354dfae818220872_Logo_wexo_bom%C3%A6rke%2Bwexo_web_white.svg" alt="" width="100" height="100" class="align-text-top">
                 </a>
                 <div class="collapse navbar-collapse" id="navbarNav">
                     <ul class="navbar-nav">
                         <li class="nav-item">
                             <a class="nav-link active" aria-current="page" href="/Index">Home</a>
                         </li>
                         <li class="nav-item">
                             <a class="nav-link" href="/Privacy">Privacy</a>
                         </li>
                         <li class="nav-item">
                             <a class="nav-link" href="#"></a>
                         </li>
                     </ul>
                 </div>
             </div>
         </nav>
     </header>


    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-buttom footer text-muted">
        <div class="container">
            &copy; 2023 - Wexo - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
